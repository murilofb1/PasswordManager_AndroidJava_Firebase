<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PasswordGeneratorv2" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/logo_background</item>
        <item name="colorPrimaryVariant">@color/logo_background</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorAccent">@color/logo_background_darker</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/logo_background_darker</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.PasswordGeneratorv2.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.PasswordGeneratorv2.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.PasswordGeneratorv2.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="Spinner">Spinner</style>
</resources>